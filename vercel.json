{
  "buildCommand": "yarn build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/",
      "destination": "/agoric-sandbox.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
